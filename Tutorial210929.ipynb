   "source": [
    "def factorial(n):\n",
    "    if type(n)==str or n<0:\n",
    "        print(\"Invalid input\")\n",
    "    elif n>=0:\n",
    "        f=1\n",
    "        i=1\n",
    "        while i<=n:\n",
    "            f*=i\n",
    "            i+=1\n",
    "        return f"
   ]
